{
  "name": "tourze/http-client-bundle",
  "description": "HTTP Client Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-filter": "*",
    "league/uri": "^7.5.1",
    "laminas/laminas-diagnostics": "^1.27",
    "nesbot/carbon": "^2.72 || ^3",
    "psr/log": "^3|^2|^1",
    "spatie/ssl-certificate": "^2.6.9",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.20 || ^3.0",
    "symfony/config": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-client": "^6.4",
    "symfony/http-client-contracts": "^2.5 | ^3.0",
    "symfony/lock": "^6.4",
    "symfony/service-contracts": "^2.5 || ^3",
    "tourze/backtrace-helper": "~0.0.5",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-async-bundle": "^0.0.6",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-user-bundle": "0.0.*",
    "tourze/doctrine-ip-bundle": "0.0.*",
    "tourze/doctrine-user-agent-bundle": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/symfony-schedule-entity-clean-bundle": "0.0.*",
    "yiisoft/json": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "HttpClientBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HttpClientBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}